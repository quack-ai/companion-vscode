{
  "name": "quack-companion",
  "displayName": "Quack Companion",
  "version": "0.0.3",
  "license": "Apache-2.0",
  "publisher": "quackai",
  "description": "Your companion for code contributions",
  "pricing": "Free",
  "author": {
    "name": "F-G Fernandez",
    "url": "https://github.com/frgfm"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Education"
  ],
  "keywords": [
    "ai",
    "assistant",
    "contribution",
    "quackai",
    "deep learning",
    "llm",
    "copilot",
    "chatgpt",
    "openai"
  ],
  "icon": "media/quack-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/quack-ai/companion"
  },
  "bugs": {
    "url": "https://github.com/quack-ai/companion/issues",
    "email": "support@quack-ai.com"
  },
  "homepage": "https://quack-ai.com",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.74.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "quack-companion",
          "title": "Quack Companion",
          "icon": "media/dark/quack.svg"
        }
      ]
    },
    "views": {
      "quack-companion": [
        {
          "type": "tree",
          "id": "quack-companion.authView",
          "name": "Authentication",
          "icon": "media/dark/quack.svg",
          "contextualTitle": "Authentication",
          "when": "!quack-companion.hasQuackToken"
        },
        {
          "type": "tree",
          "id": "quack-companion.guidelineTreeView",
          "name": "Guidelines",
          "icon": "media/dark/quack.svg",
          "contextualTitle": "Guidelines"
        }
      ]
    },
    "commands": [
      {
        "command": "quack-companion.fetchGuidelines",
        "category": "Quack AI",
        "title": "Fetch contribution guidelines",
        "icon": {
          "light": "media/light/refresh.svg",
          "dark": "media/dark/refresh.svg"
        }
      },
      {
        "command": "quack-companion.findStarterIssues",
        "category": "Quack AI",
        "title": "Find starter issues"
      },
      {
        "command": "quack-companion.defineGoal",
        "category": "Quack AI",
        "title": "Define your contribution goal"
      },
      {
        "command": "quack-companion.debugInfo",
        "category": "Quack AI",
        "title": "Get debug information"
      },
      {
        "command": "quack-companion.setEndpoint",
        "category": "Quack AI",
        "title": "Set the API endpoint"
      },
      {
        "command": "quack-companion.loginQuack",
        "category": "Quack AI",
        "title": "Log in with GitHub"
      }
    ],
    "viewsWelcome": [
      {
        "view": "quack-companion.authView",
        "contents": "Welcome to Quack extension! Let's get you started\n[Authenticate](command:quack-companion.loginQuack)",
        "when": "!quack-companion.hasQuackToken"
      }
    ],
    "keybindings": [
      {
        "command": "quack-companion.fetchGuidelines",
        "mac": "cmd+shift+g",
        "key": "ctrl+shift+g"
      },
      {
        "command": "quack-companion.debugInfo",
        "mac": "cmd+shift+d",
        "key": "ctrl+shift+d"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "lint:check": "eslint src --ext ts",
    "lint:fix": "eslint --fix src --ext ts",
    "format:check": "npx prettier . --check",
    "format:fix": "npx prettier . --write"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.2.5",
    "@types/uuid": "^9.0.2",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "prettier": "^3.0.1",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "clipboardy": "^3.0.0",
    "dotenv": "^16.3.1",
    "posthog-node": "^3.1.1",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0"
  }
}
